# Bybit API credentials
BYBIT_API_KEY=your_api_key_here
BYBIT_API_SECRET=your_api_secret_here
BYBIT_TESTNET=false

# AI Provider Configuration
AI_PROVIDER=deepseek  # "deepseek" или "openrouter"

# DeepSeek API (прямое подключение)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_BASE=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# OpenRouter API (агрегатор моделей)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_API_BASE=https://openrouter.ai/api/v1
OPENROUTER_MODEL=deepseek/deepseek-chat

# Trading parameters
MAX_TRADING_VOLUME=100
# Торговые инструменты (через запятую, без пробелов)
# Поддерживаются: BTC, ETH, SOL, TWT, MNT, XAU (золото) и другие
TRADING_PAIRS=SOLUSDT,BTCUSDT,ETHUSDT,TWTUSDT,MNTUSDT,XAUTUSDT
MAX_TRADE_AMOUNT=10
INTERVAL_MINUTES=5

# Debug/Testing flags
ENABLE_AI_ANALYSIS=true  # Включить/отключить обращение к AI для анализа
ENABLE_TRADING=true      # Включить/отключить реальную отправку ордеров в Bybit
LOG_LEVEL=INFO           # Уровень логирования: DEBUG, INFO, WARNING, ERROR
